# Define TCP buffer sizes for various networks
#   ReadMin, ReadInitial, ReadMax, WriteMin, WriteInitial, WriteMax,
    setprop net.tcp.buffersize.default 4094,87380,1220608,4096,16384,1220608
    setprop net.tcp.buffersize.wifi    4094,87380,1220608,4096,16384,1220608
    setprop net.tcp.buffersize.umts    4094,87380,1220608,4096,16384,1220608
