# Define TCP buffer sizes for various networks
#   ReadMin, ReadInitial, ReadMax, WriteMin, WriteInitial, WriteMax,
    setprop net.tcp.buffersize.default 4094,87380,404480,4096,16384,404480
    setprop net.tcp.buffersize.wifi    4094,87380,404480,4096,16384,404480
    setprop net.tcp.buffersize.umts    4094,87380,404480,4096,16384,404480
    setprop net.tcp.buffersize.hspa    4094,87380,404480,4096,16384,404480
    setprop net.tcp.buffersize.hsupa   4094,87380,404480,4096,16384,404480
    setprop net.tcp.buffersize.hsdpa   4094,87380,404480,4096,16384,404480
    setprop net.tcp.buffersize.edge    4094,87380,262144,4096,16384,262144
    setprop net.tcp.buffersize.gprs    4094,87380,262144,4096,16384,262144
    setprop net.tcp.buffersize.lte     4094,87380,404480,4096,16384,404480
    setprop net.tcp.buffersize.evdo_b  4094,87380,262144,4096,16384,262144
	
	
	
# Assign TCP buffer thresholds to be ceiling value of technology maximums
# Increased technology maximums should be reflected here.
    write /proc/sys/net/core/rmem_max  404480
    write /proc/sys/net/core/wmem_max  404480
